<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="entities_pu" transaction-type="JTA">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <jta-data-source>jdbc/odr</jta-data-source>
    <class>nl.rug.search.odr.entities.ComponentValue</class>
    <class>nl.rug.search.odr.entities.Concern</class>
    <class>nl.rug.search.odr.entities.Decision</class>
    <class>nl.rug.search.odr.entities.DecisionTemplate</class>
    <class>nl.rug.search.odr.entities.Iteration</class>
    <class>nl.rug.search.odr.entities.OprLink</class>
    <class>nl.rug.search.odr.entities.Person</class>
    <class>nl.rug.search.odr.entities.Project</class>
    <class>nl.rug.search.odr.entities.ProjectMember</class>
    <class>nl.rug.search.odr.entities.Relationship</class>
    <class>nl.rug.search.odr.entities.RelationshipType</class>
    <class>nl.rug.search.odr.entities.StakeholderRole</class>
    <class>nl.rug.search.odr.entities.State</class>
    <class>nl.rug.search.odr.entities.TemplateComponent</class>
    <class>nl.rug.search.odr.entities.Version</class>
    <class>nl.rug.search.odr.viewpoint.Handle</class>
    <class>nl.rug.search.odr.viewpoint.chronological.ChronologicalViewAssociation</class>
    <class>nl.rug.search.odr.viewpoint.chronological.ChronologicalViewNode</class>
    <class>nl.rug.search.odr.viewpoint.chronological.ChronologicalViewVisualization</class>
    <class>nl.rug.search.odr.viewpoint.relationship.RelationshipViewAssociation</class>
    <class>nl.rug.search.odr.viewpoint.relationship.RelationshipViewNode</class>
    <class>nl.rug.search.odr.viewpoint.relationship.RelationshipViewVisualization</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
    <properties>
      <property name="eclipselink.ddl-generation" value="create-tables"/>
    </properties>
  </persistence-unit>
</persistence>
